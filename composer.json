{
    "name": "solution25/maxmind",
    "description": "MaxMind fraud prevention plugin for Shopware 6",
    "type": "shopware-platform-plugin",
    "version": "1.2.1",
    "license": "MIT",
    "require": {
        "shopware/core": "~6.6.0",
        "maxmind/minfraud": "^3.2",
        "shopware/storefront": "~6.6.0"
    },
    "extra": {
        "shopware-plugin-class": "MaxMind\\MaxMind",
        "label": {
            "de-DE": "MaxMind Bestellbetrugserkennung",
            "en-GB": "MaxMind Order Fraud Detection"
        },
        "description": {
            "de-DE": "Diese MaxMind-Integration erkennt und verhindert automatisch betr√ºgerische Bestellungen in Ihrem Shopware-6-Shop. Analysiert Bestellungen in Echtzeit anhand von Risikofaktoren.",
            "en-GB": "This MaxMind integration automatically detects and prevents fraudulent orders in your Shopware 6 store by analyzing transactions in real time with advanced risk scoring."
        },
        "manufacturerLink": {
            "de-DE": "https://solution25.com/",
            "en-GB": "https://solution25.com/en/home_en/"
        },
        "supportLink": {
            "de-DE": "https://solution25.com/",
            "en-GB": "https://solution25.com/en/home_en/"
        }
    },
    "authors": [
        {
            "name": "Solution25",
            "email": "info@solution25.com",
            "homepage": "https://solution25.com/"
        }
    ],
    "autoload": {
        "psr-4": {
            "MaxMind\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaxMind\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/runtime": true
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@enable-prepush"
        ],
        "post-update-cmd": [
            "@enable-prepush"
        ],
        "enable-prepush": [
            "cp ./pre-push .git/hooks/pre-push",
            "chmod +x .git/hooks/pre-push"
        ]
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^4.0"
    }
}
